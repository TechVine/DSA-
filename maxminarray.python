#python program to find maximum and minimum of array
arr=[1,2,3,4,10,68]

max=arr[0]
n=len(arr)

for i in range(1,n):
    if arr[i]>max:
        max=arr[i]
        
print("Maximum element is :" ,max)

arr=[30,20,15,10,2]
min=arr[0]
n=len(arr)

for i in range(1,n):
    if arr[i]<min:
        min=arr[i]
print("Minimum element is :" ,min)
